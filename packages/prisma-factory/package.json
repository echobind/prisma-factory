{
  "name": "prisma-factory",
  "version": "0.1.13",
  "description": "Generates factories for your prisma models",
  "repository": {
    "type": "git",
    "url": "git@github.com:echobind/prisma-factory.git",
    "directory": "packages/prisma-factory"
  },
  "contributors": [],
  "license": "MIT",
  "keywords": [
    "prisma",
    "generator",
    "factory",
    "factories"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./bin/generator.js",
  "scripts": {
    "dev": "tsup src/index.ts src/generator.ts --format cjs,esm --watch --onSuccess \"pnpm prisma:generate\"",
    "build": "tsup src/index.ts src/generator.ts --format cjs,esm --dts-resolve",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "clean": "rimraf .turbo node_modules dist prisma/client prisma/generated",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "migrate:resolve": "prisma migrate resolve",
    "db:generate": "prisma generate",
    "db:start": "docker-compose up -d",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "@prisma/client": ">=3.0.0",
    "typescript": ">4.5.2"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.8.0",
    "@prisma/internals": "^4.8.0",
    "camel-case": "^4.1.2",
    "ts-morph": "^17.0.1"
  },
  "devDependencies": {
    "@prisma-factory/config": "workspace:^",
    "@prisma-factory/tsconfig": "workspace:^",
    "@prisma/client": "^4.8.0",
    "@types/chance": "^1.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "chance": "^1.1.8",
    "eslint": "^8.11.0",
    "execa": "^6.1.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "prettier-plugin-prisma": "^3.11.0",
    "prisma": "^4.7.1",
    "release-it": "^14.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "tslib": "^2.3.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3",
    "vitest": "^0.6.3"
  }
}
